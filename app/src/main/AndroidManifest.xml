<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.music_o_player">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".all_songs"
            android:label="All Songs"/>
        <activity android:name=".artists"
            android:label="Artists"/>
        <activity android:name=".favourites"
            android:label="Favourites"/>
        <activity android:name=".categories"
            android:label="Categories"/>
        <activity android:name=".now_playing"
            android:label="Now Playing"/>
    </application>

</manifest>